<link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css">
<script>
 $(document).ready(function() {
        var table = $('#example').DataTable();
      }
  );
</script>
<br>
<div class="ui tabular menu">
  <a class="item">
    {{#link-to "exercise"}}Exercises{{/link-to}}
  </a>
  <a class="item">
    {{#link-to "rehab-plans"}}Create a Plan{{/link-to}}
  </a>
  <a class="active item">
      {{#link-to "assessment-tests"}}Complete Assessment Test{{/link-to}}
  </a>
</div>
<div class="rehabPlan">
    <h2>Assesment Tests</h2>
    <table id="example" class="ui celled table" width="100%" cellspacing="0">
        <thead>
            <tr>
                <th>Name</th>
                <th>Description</th>
                <!-- <th>Form</th> -->
                <th>Complete</th>
            </tr>
        </thead>
        <tfoot>
            <tr>
                <th>Name</th>
                <th>Description</th>
                <!-- <th>Form</th> -->
                <th>Complete</th>
            </tr>
        </tfoot>
        <tbody>
            {{#each model as |onePost|}}
            <tr>
                <td>{{onePost.name}}</td>
                <td>{{onePost.description}}</td>
                <!-- <td>{{onePost.form}}</td> -->

                <td>
                    {{complete-test ID=onePost.id}}
                </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>
