</div>
<div style="margin-left: 4em; margin-right: 4em">
    <h2>Completed Injury Forms</h2>
</div>

<div class="container">
    <div class="row">
        <div class="col-lg-12" style="margin: 30px;">
            <table id="example" class="ui celled table" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Email</th>
                        <th>Injury Form Name</th>
                        <th>View</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Email</th>
                        <th>Injury Form Name</th>
                        <th>View</th>
                    </tr>
                </tfoot>
                <tbody>
                    {{#each model as |completedInjuryForm|}}
                      <tr>
                      <td>{{completedInjuryForm.email}}</td>
                      <td>{{completedInjuryForm.injuryForm.name}}</td>
                      <td>
                        {{show-injury-form ID=completedInjuryForm.id completedInjuryForm=completedInjuryForm}}
                      </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>

<script>
$(document).ready(function() {
       var table = $('#example').DataTable();
     }
 );
</script>