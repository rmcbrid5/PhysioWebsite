<link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css">
<br>
<div class="ui tabular menu">
    <a class="item">
        {{#link-to 'physio-profile'}}Profile{{/link-to}}
    </a>
  <a class="item">
    {{#link-to "exercise"}}Exercises{{/link-to}}
  </a>
  <a class="item">
    {{#link-to "rehab-plans"}}Rehabilitation Plans{{/link-to}}
  </a>
  <a class="item">
    {{#link-to "patient-profiles"}}Assign Rehabilitation Plans{{/link-to}}
  </a>
  <a class="active item">
      {{#link-to "physio-completed-injury-forms"}}Completed Injury Forms{{/link-to}}
  </a>
  <a class="item">
    {{#link-to "patient-images"}}User's Images{{/link-to}}
  </a>
  <a class="item">
    {{#link-to "appointments"}}Upcoming Appointments{{/link-to}}
