<div class="rehabPlan">
  <h2>User's Images</h2>
  <div class="row">
    <div class="col-lg-12">
      <table id="example" class="ui celled table" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Email</th>
            <th>View Images</th>
          </tr>
        </thead>
        <tfoot>
          <tr>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Email</th>
            <th>View Images</th>
          </tr>
        </tfoot>
        <tbody>
          {{#each model as |onePost|}}
          <tr>
            <td>{{onePost.familyName}}</td>
            <td>{{onePost.givenName}}</td>
            <td>{{onePost.email}}</td>
            <td>{{view-images ID=onePost.id}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>