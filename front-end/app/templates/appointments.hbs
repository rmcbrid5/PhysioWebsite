<link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css">
<br>
<div class="ui tabular menu">
    <a class="item">
        {{#link-to 'physio-profile'}}Profile{{/link-to}}
    </a>
  <a class="item">
    {{#link-to "exercise"}}Exercises{{/link-to}}
  </a>
  <a class="item">
    {{#link-to "rehab-plans"}}Rehabilitation Plans{{/link-to}}
  </a>
  <a class="item">
    {{#link-to "patient-profiles"}}Assign Rehabilitation Plans{{/link-to}}
  </a>
  <a class="item">
      {{#link-to "physio-completed-injury-forms"}}Completed Injury Forms{{/link-to}}
  </a>
  <a class="item">
    {{#link-to "patient-images"}}User's Images{{/link-to}}
  </a>
  <a class="active item">
    {{#link-to "appointments"}}Upcoming Appointments{{/link-to}}
  </a>
  <a class="item">
    {{#link-to 'generate-reports'}}Generate Patient Reports{{/link-to}}
  </a>
</div>
<div class="rehabPlan">
  <h2>Appointments</h2>
  <div class="row">
    <div class="col-lg-12">
      <table id="example" class="ui celled table" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>Name</th>
            <th>Time</th>
            <th>Reason</th>
          </tr>
        </thead>
        <tfoot>
          <tr>
            <th>Name</th>
            <th>Time</th>
            <th>Reason</th>
          </tr>
        </tfoot>
        <tbody>
          {{#each model as |onePost|}}
          <tr>
            <td>{{onePost.patientProfile.givenName}} {{onePost.patientProfile.familyName}}</td>
            <td>{{onePost.date}}</td>
            <td>{{onePost.reason}} {{appt-button}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>